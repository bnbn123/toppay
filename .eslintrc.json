{
  "plugins": ["prettier"],
  "extends": [
    "next/core-web-vitals",
    "react-app",
    "react-app/jest",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
    "prettier/prettier": 2
  }
}
